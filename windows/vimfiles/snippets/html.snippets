snippet navtop
	.nav-top { position: relative; padding: .2em; zoom: 1;border:2px solid red;background:none;}
	.nav-top .ui-tabs-nav { margin: 0; padding: 0; background:white; border:3px solid green;}
	.nav-top .ui-tabs-panel {  padding: 0em 0em; background:white; border:3px solid blue;}
	.nav-top .ui-tabs-nav li { float:right;list-style: none; position: relative; top:0px;
	    margin:  0; border: 0 ;border-left: red solid 1px; padding: 0; white-space: nowrap; background:none;
	   /* border:red 2px solid*/}
	.nav-top .ui-tabs-nav li a { float:right; padding: .5em 1em; text-decoration: none; }
	.nav-top .ui-tabs-nav li.ui-tabs-selected { margin-bottom: 0; padding-bottom: 0px; }
	.nav-top .ui-tabs-nav li.ui-tabs-selected a, .nav-top .ui-tabs-nav li.ui-state-disabled a, .nav-top .ui-tabs-nav li.ui-state-processing a {
	    cursor: text; }
	.nav-top .ui-tabs-nav li a, .ui-tabs.ui-tabs-collapsible .ui-tabs-nav li.ui-tabs-selected a { cursor: pointer; }
	.nav-top .ui-tabs-hide { display: none !important; }
	.nav-top .ui-tabs-nav li a:focus{ outline:none; }
	.nav-top .ui-tabs-selected { background:blue;}
