	.nav-left .ui-tabs-panel {float:right; border-width: 0; padding: 0em 0em; background: none; border:1px solid white;width:${2:80%};}
	.nav-left .ui-tabs-nav li { float:none;list-style: none;  position: relative; top: 0px;
